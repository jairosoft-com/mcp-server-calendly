{
  "name": "mcp-server-calendly",
  "version": "1.0.0",
  "description": "MCP server for Calendly integration, providing tools to fetch and manage Calendly events.",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "calendly": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "jest",
    "test:calendly": "ts-node --esm src/test-calendly.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "calendly",
    "mcp",
    "scheduler",
    "calendar",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.32",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "axios": "^1.6.8",
    "zod": "^3.25.67"
  }
}
